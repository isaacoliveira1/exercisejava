		Cliente c2 = new Cliente();
		c1.setnome("Bethoven Kyen");
		c1.setemail("bethokyen@hotmail.com");
		c1.setcodigo(98);
		c1.setfone(98437654);
		c2.setnome("Pedro Horte");
		c2.setemail("darthvader2398@hotmail.com");
		c2.setcodigo(367);
		c2.setfone(94342387);
