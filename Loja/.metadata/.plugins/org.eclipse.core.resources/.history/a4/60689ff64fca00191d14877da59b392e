package Atributos;

public class Multa {
	private String placa;
	private String tipoMulta;
	private String data;
	private String vencimento;
	
	public Multa(String placa, String tipoMulta, String data, String vencimento) {
		this.placa = placa;
		this.tipoMulta = tipoMulta;
		this.data = data;
		this.vencimento = vencimento;
		
	}
}
