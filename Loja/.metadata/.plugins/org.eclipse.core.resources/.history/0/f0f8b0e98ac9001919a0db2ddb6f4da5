package Atributos;

public class Recipiente {
	private int cp;
	private int qt;
	
	public Recipiente() {
		
	}
	public Recipiente(int cp, int qt) {
		this.cp = cp;
		this.qt = qt;
	}
			public void setcp(int cp) {
				this.cp = cp;
			}
			public void alteracp(int cp) {
				this.cp = cp;
			}
			public int getcp() {
				return this.cp;
			}
					public void setqt(int qt) {
						this.qt = qt;
					}
						public int getqt() {
							return this.qt;
							
						}
}
